{
  "name": "@angular-package/docs",
  "version": "0.1.11",
  "description": "Angular 5+ module to create documentation.",
  "author": "Angular Package <angular-package@wwwdev.io> (http://angular-package.wwwwdev.io/docs)",
  "homepage": "https://github.com/angular-package/angular-package/tree/master/packages/docs#readme",
  "main": "./dist/bundle.umd.js",
  "module": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "contributors": [
    "Angular Package <angular-package@wwwdev.io>"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "rimraf ./dist ./node_modules",
    "bundle": "rollup -c rollup.config.js --environment BUILD:production",
    "bundle:dev": "rollup -c rollup.config.js --environment BUILD:development",
    "prebuild": "npm install",
    "build": "./node_modules/.bin/ngc -p tsconfig.json",
    "postbuild": "npm run copyfiles:all",
    "prepublish": "",
    "start": "npm run build && npm run bundle",
    "clean:start": "npm run clean && npm start",
    "clean:start:pack": "npm run clean && npm run clean:start && npm pack",
    "copyfiles:api": "copyfiles ./api/src/*.html ./api/src/*.scss ./api/src/*.css ./dist",
    "copyfiles:example": "copyfiles ./example/src/*.html ./example/src/*.scss ./example/src/*.css ./dist",
    "copyfiles:all": "npm run copyfiles:example && npm run copyfiles:api",
    "test": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@angular-package/prism": "^2.0.0",
    "@angular/animations": "^5.1.2",
    "@angular/cdk": "5.0.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/material": "^5.0.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@ngx-markdown/core": "^0.2.2",
    "@types/jasmine": "^2.8.3",
    "@types/karma": "^1.7.3",
    "@types/node": "^8.5.2",
    "@types/prismjs": "^1.9.0",
    "clean-css": "^4.1.9",
    "codelyzer": "^4.0.2",
    "copyfiles": "^1.2.0",
    "html-minifier": "^3.5.8",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-rollup-preprocessor": "^5.0.2",
    "node-sass": "^4.7.2",
    "prismjs": "^1.9.0",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.0",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.6",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "uglify-es": "^3.3.2",
    "zone.js": "^0.8.19"
  },
  "peerDependencies": {
    "@angular/cdk": "5.0.2",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/material": "^5.0.2",
    "@angular-package/prism": "^2.0.0",
    "@ngx-markdown/core": "^0.2.2",
    "@types/prismjs": "^1.9.0",
    "prismjs": "^1.9.0"
  },
  "keywords": [
    "angular-package docs example api",
    "angular-package",
    "angular-package/docs",
    "@angular-package",
    "@angular-package docs",
    "@angular-package/docs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angular-package/angular-package.git"
  },
  "bugs": {
    "url": "https://github.com/angular-package/angular-package/issues"
  }
}
