{
  "name": "@angular-package/form",
  "version": "0.1.0",
  "description": "Dynamic forms or html form elements.",
  "author": "Angular Package <angular-package@wwwdev.io> (http://angular-package.wwwwdev.io/form)",
  "homepage": "https://github.com/angular-package/angular-package#readme",
  "main": "./bundle.umd.js",
  "module": "./index.js",
  "jsnext:main": "./index.js",
  "typings": "./index.d.ts",
  "contributors": [
    "Angular Package <angular-package@wwwdev.io>"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "rimraf ./element ./interface ./material ./type ./node_modules ./index.* ./bundle.umd.*",
    "bundle": "rollup -c rollup.config.js --environment BUILD:production",
    "bundle:dev": "rollup -c rollup.config.js --environment BUILD:development",
    "prebuild": "npm install",
    "build": "./node_modules/.bin/ngc -p tsconfig.json",
    "postbuild": "npm run copyfiles:all",
    "prepublish": "",
    "start": "npm run build && npm run bundle",
    "clean:start": "npm run clean && npm start",
    "clean:start:pack": "npm run clean && npm run clean:start && npm pack",
    "copyfiles:element": "cd ./packages/element && copyfiles ./src/*.html ./src/*.scss ./src/*.css ./../../element",
    "copyfiles:all": "npm run copyfiles:element ",
    "test": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@angular-package/core": "^1.0.1",
    "@angular/animations": "^5.0.2",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/compiler-cli": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@types/jasmine": "^2.8.2",
    "@types/karma": "^1.7.2",
    "@types/lodash": "^4.14.85",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^8.0.53",
    "codelyzer": "^4.0.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.1",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-rollup-preprocessor": "^5.1.1",
    "node-sass": "^4.8.3",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.8",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.51.8",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "uglify-es": "^3.1.10",
    "zone.js": "^0.8.18"
  },
  "keywords": [
    "Angular",
    "Component",
    "Form"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-package/forms.git"
  },
  "bugs": {
    "url": "https://github.com/ngx-package/forms/issues"
  }
}
