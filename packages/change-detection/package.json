    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.6",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "uglify-es": "^3.3.5",
    "zone.js": "^0.8.20"
  },
